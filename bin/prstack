#!/usr/bin/env bb

(def script-dir (-> *file*
                  java.io.File.
                  .getParent
                  java.io.File.
                  .getParent))

(require '[babashka.classpath :as cp])
(cp/add-classpath (str script-dir "/src"))

(require
  '[prstack.utils :as utils]
  '[prstack.commands :as commands]
  '[prstack.main :as main])

(main/run! *command-line-args*)
